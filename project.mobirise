          "_name": "header1",
          "_customHTML": "<section class=\"carousel slide testimonials-slider\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"BootstrapCarouselSwipe,mbr-testimonials-slider,PlayerVimeo,font-awesome\" data-interval=\"false\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" title=\"Padding Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"0\" inline=\"\">\n        <input type=\"range\" title=\"Padding Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"0\" inline=\"\">\n        <input type=\"range\" title=\"Height Content\" name=\"heightContent\" min=\"400\" max=\"1000\" value=\"819\" step=\"20\">\n\n\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Button\" name=\"showButtons\" checked=\"\">\n        <input type=\"checkbox\" title=\"Video\" name=\"showVideo\" checked=\"\">\n        <input type=\"checkbox\" title=\"Bottom Block\" name=\"showBottomBlock\" checked=\"\">\n        <header>Additional</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"showAutoplay\" image-element=\"\">\n        <input type=\"range\" inline=\"\" title=\"Autoplay Interval (ms)\" name=\"interval\" min=\"1000\" max=\"8000\" step=\"1000\" value=\"5000\" condition=\"showAutoplay\">\n\n        <select title=\"Count Items\" name=\"testimonialsSlides\">\n          <option value=\"1\">1</option>\n          <option value=\"2\">2</option>\n          <option value=\"3\" selected=\"\">3</option>\n          <option value=\"4\">4</option>\n          <option value=\"5\">5</option>\n          <option value=\"6\">6</option>\n          <option value=\"7\">7</option>\n          <option value=\"8\">8</option>\n        </select>\n\n        <header>Video</header>\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#354463\">\n        <input type=\"color\" title=\"Icon Border Color\" name=\"iconBGColor\" value=\"#ffffff\">\n        <input type=\"image\" name=\"video\" value=\"@PROJECT_PATH@/assets/images/attorney5.jpg\" title=\"Background Image\">\n        <input type=\"text\" title=\"Modal Video\" name=\"modalVideo\" value=\"https://vimeo.com/94651406\">\n\n\n        <header>Footer</header>\n        <input type=\"range\" inline=\"\" title=\"Col 1 Items\" name=\"menuItems3\" min=\"1\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Col 2 Items\" name=\"menuItems4\" min=\"1\" max=\"10\" step=\"1\" value=\"1\">\n  \n        <input type=\"range\" inline=\"\" title=\"Social Items\" name=\"socialItems\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n      \n\n        <header>Color</header>\n       \n        <input type=\"color\" title=\"Icons Color\" name=\"icons\" value=\"#ffffff\" selected=\"\" condition=\"socialItems > 0\">\n        <input type=\"color\" title=\"Arrow Color\" name=\"arrowColor\" value=\"#a5d3be\">\n        <input type=\"color\" title=\"Arrow Hover Color\" name=\"arrowColorHover\" value=\"#354463\">\n        <input type=\"color\" title=\"Color\" name=\"bgColor\" value=\"#2A503F\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"bgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\">\n\n        <input type=\"color\" title=\"Video Color\" name=\"bgColor2\" value=\"#354463\">\n        <input type=\"range\" inline=\"\" title=\"Video Opacity\" name=\"bgOpacity2\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n          <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/attorney1.jpg\">\n          <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n          <input type=\"video\" title=\"Background Video\" value=\"https://vimeo.com/280886447\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n  \n\n        <div class=\"carousel slide \" role=\"listbox\" data-pause=\"true\" data-keyboard=\"false\" data-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-interval=\"{{showAutoplay ? interval : 'false'}}\">\n            <div mbr-list=\"\" mbr-list-grow=\"testimonialsSlides\" class=\"carousel-inner \">\n                <div class=\"carousel-item wrap\">\n                    <div class=\"media-container-row\">\n                        <div class=\"col-lg-12 image-element align-self-stretch\">\n                          <div class=\"img-wrap\">\n                            <div class=\"card-box\">\n                        \n                            </div>\n                              <img src=\"@PROJECT_PATH@/assets/images/attorney1.jpg\">\n                          </div>\n                        </div>\n                        <div class=\"col-lg-12 text-element\">\n                            <div class=\"container text-content\">\n                              <div class=\"col-lg-12 block-wrap\">\n                            